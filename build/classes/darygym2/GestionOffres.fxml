<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="645.0" prefWidth="816.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="darygym2.GestionOffresController">
    <stylesheets>
        <URL value="@gestionoffres.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tableoffres" layoutX="344.0" layoutY="151.0" onMouseClicked="#selected" prefHeight="258.0" prefWidth="348.0">
        <columns>
          <TableColumn fx:id="imagea" prefWidth="75.0" text="image" />
          <TableColumn fx:id="titrea" prefWidth="75.0" text="titre" />
            <TableColumn fx:id="descriptiona" prefWidth="101.0" text="description" />
            <TableColumn fx:id="prixa" prefWidth="96.0" text="prix" />
        </columns>
      </TableView>
      <TextField fx:id="tfimage" layoutX="93.0" layoutY="182.0" promptText="veuillez entrer une image" />
      <TextField fx:id="tftitre" layoutX="93.0" layoutY="242.0" />
      <TextField fx:id="tfdescription" layoutX="93.0" layoutY="317.0" />
      <TextField fx:id="tfprix" layoutX="93.0" layoutY="394.0" />
      <Button fx:id="btnsupprimer" layoutX="478.0" layoutY="472.0" mnemonicParsing="false" onAction="#supprimerOffres" prefHeight="59.0" prefWidth="149.0" text="supprimer" />
      <Button fx:id="btnmodifier" layoutX="280.0" layoutY="472.0" mnemonicParsing="false" onAction="#modifierOffres" prefHeight="59.0" prefWidth="149.0" text="modifier" />
      <Button fx:id="btnajouter" layoutX="61.0" layoutY="472.0" mnemonicParsing="false" onAction="#ajouterOffres" prefHeight="59.0" prefWidth="166.0" text="ajouter" />
   </children>
</AnchorPane>
