<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="darygym2.GestionAbonnementController">
    <stylesheets>
        <URL value="@gestionabonnement.css" />
    </stylesheets>
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="645.0" prefWidth="816.0" styleClass="mainFxmlClass">
         <stylesheets>
            <URL value="@gestionoffres.css" />
         </stylesheets>
         <children>
            <TableView fx:id="tableabonnement" layoutX="392.0" layoutY="151.0" onMouseClicked="#selected" prefHeight="258.0" prefWidth="291.0">
               <columns>
                  <TableColumn fx:id="offrea" prefWidth="138.0" text="offre" />
                  <TableColumn fx:id="dureea" prefWidth="152.0" text="duree" />
               </columns>
            </TableView>
            <TextField fx:id="tfoffre" layoutX="14.0" layoutY="221.0" prefHeight="31.0" prefWidth="322.0" promptText="veuillez entrer une l'offre de l'abonnement" />
            <TextField fx:id="tfduree" layoutX="14.0" layoutY="300.0" prefHeight="31.0" prefWidth="322.0" promptText="veuillez entrer la duree de l'abonnement" />
            <Button fx:id="btnsupprimer" layoutX="478.0" layoutY="472.0" mnemonicParsing="false" onAction="#supprimerAbonnement" prefHeight="59.0" prefWidth="149.0" text="supprimer" />
            <Button fx:id="btnmodifier" layoutX="280.0" layoutY="472.0" mnemonicParsing="false" onAction="#modifierAbonnement" prefHeight="59.0" prefWidth="149.0" text="modifier" />
            <Button fx:id="btnajouter" layoutX="61.0" layoutY="472.0" mnemonicParsing="false" onAction="#ajouterAbonnement" prefHeight="59.0" prefWidth="166.0" text="ajouter" />
            <ComboBox fx:id="affecter" layoutX="9.0" layoutY="136.0" prefHeight="31.0" prefWidth="322.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
